// Prettier: https://prettier.io/playground/
// Options Â· Prettier: https://prettier.io/docs/en/options
{
  "experimentalTernaries": true,
  "printWidth": 160,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": false,
  // "arrowParens": "avoid",
  "rangeStart": 0,
  "rangeEnd": Infinity,
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": true,
  "endOfLine": "crlf",
  "embeddedLanguageFormatting": "auto",
  "singleAttributePerLine": false
}
